    "# Itertools\n",
    "import itertools\n",
    "\n",
    "# infinite counting\n",
    "for x in itertools.count(50):\n",
    "    print(x)\n",
    "    if x == 80:\n",
    "        break;\n",
    "\n",
    "# infinite cycling\n",
    "x = 0;\n",
    "for c in itertools.cycle(\"RACECAR\"):\n",
    "    print(c)\n",
    "    x = x+1\n",
    "    if x > 50:\n",
    "        break\n",
    "        \n",
    "x = 0;\n",
    "for c in itertools.cycle([1,2,3,4]):\n",
    "    print(c)\n",
    "    x = x+1\n",
    "    if x > 50:\n",
    "        break\n",
    "\n",
    "# infinite repeating\n",
    "for r in itertools.repeat(True):\n",
    "    print(r)\n",
